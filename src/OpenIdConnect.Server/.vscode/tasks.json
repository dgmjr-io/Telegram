{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build",
      "command": "dotnet",
      "type": "process",
      "args": [
        "build",
        "${workspaceFolder}/Telegram.OpenIdConnect.Server.csproj",
        "/property:GenerateFullPaths=true",
        "/consoleloggerparameters:NoSummary;ForceNoAlign"
      ],
      "problemMatcher": "$msCompile"
    },
    {
      "label": "publish",
      "command": "dotnet",
      "type": "process",
      "args": [
        "publish",
        "${workspaceFolder}/Telegram.OpenIdConnect.Server.csproj",
        "/property:GenerateFullPaths=true",
        "/consoleloggerparameters:NoSummary;ForceNoAlign"
      ],
      "problemMatcher": "$msCompile"
    },
    {
      "label": "watch",
      "command": "dotnet",
      "type": "process",
      "args": [
        "watch",
        "run",
        "--project",
        "${workspaceFolder}/Telegram.OpenIdConnect.Server.csproj"
      ],
      "problemMatcher": "$msCompile"
    },
    {
      "label": "Run Telegram.OpenIdConnect.Server.Server",
      "command": "dotnet",
      "type": "process",
      "args": [
        "run",
        "--project",
        "${workspaceFolder}/Telegram.OpenIdConnect.Server.csproj",
        "/property:GenerateFullPaths=true",
        "/consoleloggerparameters:NoSummary;ForceNoAlign",
        "-c:Local"
      ],
      "problemMatcher": "$msCompile"
    },
    {
      "label": "ngrok - Telegram OpenIdConnect",
      "type": "shell",
      "command": "ngrok",
      "args": [
        "tunnel",
        "--label",
        "edge=edghts_2YVlve6yoIHeHG4dzj3cV1Ojnqn",
        "https://localhost:7003"
      ],
      "problemMatcher": []
    }
  ]
}
